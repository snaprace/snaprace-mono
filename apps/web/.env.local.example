# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Auth Configuration
AUTH_SECRET=your_auth_secret_here
AUTH_URL=http://localhost:3000
CRISP_WEBSITE_ID=your_crisp_website_id_here

# Development Subdomain Testing
# Set this to test a specific organization subdomain in local development
# Example: millenniumrunning
NEXT_PUBLIC_DEV_SUBDOMAIN=millenniumrunning

# Google Analytics (Optional)
# NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Microsoft Clarity (Optional)
# NEXT_PUBLIC_CLARITY_PROJECT_ID=your_clarity_id_here
